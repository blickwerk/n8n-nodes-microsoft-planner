{
  "name": "@blickwerk/n8n-nodes-microsoft-planner",
  "version": "1.4.0",
  "description": "n8n node for Microsoft Planner - Create and retrieve tasks",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "microsoft",
    "planner",
    "tasks"
  ],
  "license": "MIT",
  "homepage": "https://github.com/blickwerk/n8n-nodes-microsoft-planner",
  "author": {
    "name": "Blickwerk Media UG",
    "email": "info@blickwerk.media"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/blickwerk/n8n-nodes-microsoft-planner.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/MicrosoftPlannerOAuth2Api.credentials.js"
    ],
    "nodes": [
      "dist/nodes/MicrosoftPlanner/MicrosoftPlanner.node.js"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^5.59.0",
    "eslint": "^8.40.0",
    "eslint-plugin-n8n-nodes-base": "^1.12.0",
    "gulp": "^4.0.2",
    "n8n-workflow": "^1.0.0",
    "prettier": "^2.8.8",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "n8n-core": "^1.0.0"
  },
  "peerDependencies": {
    "n8n-workflow": "^1.0.0"
  }
}
